class <%= @name %>ViewModel: <%= @name %>ViewModelDelegate {
  
  var model: [<%= @name %>DataItem]? {
      didSet {
          viewDelegate?.modelDidChange(viewModel: self)
      }
  }
  weak var viewDelegate: <%= @name %>ViewModelViewDelegate?
  weak var coordinatorDelegate: <%= @name %>ViewModelCoordinatorDelegate?

  internal var title: String {
        return "<%= @name %>"
    }

}